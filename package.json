{
  "name": "react-snap-ts",
  "version": "1.0.0",
  "description": "A typescript implementation of react snap with some additional features and bug fixes",
  "main": "dist/index.js",
  "bin": {
    "react-snap": "dist/run.js"
  },
  "files": [
    "dist/",
    "vendor/"
  ],
  "author": "stereobooster",
  "contributors": [
    "jan-rycko"
  ],
  "license": "MIT",
  "repository": "jan-rycko/react-snap",
  "engines": {
    "node": ">= 12.22.11"
  },
  "scripts": {
    "toc": "yarn run markdown-toc -i doc/recipes.md",
    "pretest": "npm run build",
    "test": "jest",
    "build": "tsc",
    "prepublish": "npm run build",
    "precommit": "prettier --write {*,src/*}.{js,json,css}",
    "force-resolutions": "npx --yes npm-force-resolutions"
  },
  "resolutions": {
    "node-fetch": "2.6.7",
    "nth-check": "2.0.1"
  },
  "dependencies": {
    "clean-css": "5.3.3",
    "express": "4.18.2",
    "express-history-api-fallback": "2.2.1",
    "html-minifier": "4.0.0",
    "minimalcss": "0.11.3",
    "mkdirp": "^3.0.1",
    "puppeteer": "^21.7.0",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-block-resources": "^2.4.3",
    "serve-static": "1.15.0",
    "setimmediate": "^1.0.5",
    "shelljs": "^0.8.5",
    "sourcemapped-stacktrace-node": "2.1.8"
  },
  "devDependencies": {
    "@types/clean-css": "^4.2.11",
    "@types/express": "^4.17.21",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/jest": "^29.5.11",
    "@types/puppeteer": "^7.0.4",
    "@types/shelljs": "^0.8.15",
    "dev-null-stream": "0.0.1",
    "jest": "^29.7.0",
    "markdown-toc": "1.2.0",
    "prettier": "3.2.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
